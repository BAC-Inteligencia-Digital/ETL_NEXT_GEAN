                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT Id_Oportunidad,Fecha_creacion,'Guatemala' as pais,

CASE WHEN [Campana] IS NULL THEN ''
     WHEN [Campana] = '-- NO DATA --' THEN ''
	 WHEN [Campana] LIKE 'GUA%' THEN REPLACE([Campana], ',', '/')
	 ELSE [Campana] END AS BACID,
	 
	 CASE WHEN [Medio] IS NULL THEN 'orgánicas'
	      WHEN [Campana] IS NULL THEN 'orgánicas'
		  WHEN [Campana] = '-- NO DATA --' THEN 'orgánicas'
		  ELSE [Medio] END AS Medio,
	 
CASE WHEN [Campana] IS NULL THEN ''
     WHEN [Campana] = '-- NO DATA --' THEN ''
	 WHEN [Campana] LIKE 'GUA%' THEN REPLACE([Campana], ',', '/')
	 ELSE [Campana] END AS CAMPANA,
	 
CASE WHEN [LANDING PAGE] is null THEN '' 
     WHEN [LANDING PAGE] = '-- NO DATA --' THEN '' 
	 ELSE [LANDING PAGE] END as Landing_page,
	 
