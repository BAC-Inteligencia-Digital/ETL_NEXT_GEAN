                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT CaseID, fecha,pais, 

	   CASE WHEN medio in ('bacid','bacid_at') then REPLACE(bacid,',','/') end as bacid,
	   CASE WHEN medio in ('bacid','bacid_at') then REPLACE(campana,',','/') end as campana,

	   CASE WHEN medio is null then 'orgánicas' else medio end as medio,

	   CASE WHEN (medio in ('bacid','bacid_at') and campana like '%&amp;%') 
	        then SUBSTRING(
            campana, 
		    CHARINDEX('&amp;', campana)+1, 
            LEN(campana)-CHARINDEX('&amp;', campana)
            ) end as click_id

FROM (

select CaseID, cast(CaseIDCreateDateTime_HourCR as date) as fecha, 'Guatemala' as pais,

       SUBSTRING(OPABacId,CHARINDEX('=',OPABacId)+1,LEN(OPABacId)) as bacid,
	   
	   SUBSTRING (OpaBacId,0,CHARINDEX('=',OPABacId)) as medio,

	   SUBSTRING(OPABacId,CHARINDEX('=',OPABacId)+1,LEN(OPABacId)) as campana 

from [BULK_CLM_REG].[pyWorkPage].[tblWorkPage]
Where Pais = 'GUA'
AND (CaseID LIKE '%SX%' OR CaseID LIKE '%BE%')
AND ProductName = 'Credit Card'                                                                             
AND CaseIdCreateDateTime_HourCR &gt;= CONVERT(varchar,DATEADD(DAY,-25,getDate()),23)

) AS TT</property>
