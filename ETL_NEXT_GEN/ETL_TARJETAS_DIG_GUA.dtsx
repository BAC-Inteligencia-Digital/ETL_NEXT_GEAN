                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT ID_OPORTUNIDAD, FECHA_CREACION, PAIS, [URL ACTUAL], 

CASE WHEN [CAMPANA] IS NULL THEN ''
     WHEN [CAMPANA] = '-- NO DATA --' THEN ''
	 WHEN [CAMPANA] LIKE 'GUA%' THEN REPLACE([CAMPANA], ',', '/')
	 WHEN [CAMPANA] LIKE 'REG%' THEN REPLACE([CAMPANA], ',', '/')
	 ELSE [CAMPANA] END AS BACID,
	 
	 CASE WHEN [MEDIO] IS NULL THEN 'orgánicas'
	      WHEN [CAMPANA] IS NULL THEN 'orgánicas'
		  WHEN [CAMPANA] = '-- NO DATA --' THEN 'orgánicas'
		  ELSE [MEDIO] END AS MEDIO,
	 
CASE WHEN [CAMPANA] IS NULL THEN ''
     WHEN [CAMPANA] = '-- NO DATA --' THEN ''
	 WHEN [CAMPANA] LIKE 'GUA%' THEN REPLACE([Campana], ',', '/')
	 WHEN [CAMPANA] LIKE 'REG%' THEN REPLACE([Campana], ',', '/')
	 ELSE [CAMPANA] END AS CAMPANA, 
	 
CASE WHEN [LANDING PAGE] IS NULL THEN '' 
     WHEN [LANDING PAGE] = '-- NO DATA --' THEN '' 
	 ELSE [LANDING PAGE] END AS LANDINGPAGE,

CASE WHEN [CLICK ID] = '-- NO DATA --' THEN NULL
     ELSE [CLICK ID] END AS CLICK_ID

FROM (

Select

a.OpptyNumber as ID_OPORTUNIDAD,
CAST(a.CreationDate as date) as FECHA_CREACION,
'Guatemala' as PAIS,
b.Nombre_c,
b.Valor_c

from [BULK_ENGAGEMENT_CLOUD_REG].[GTM].[tblOportunidades] as a
left join [BULK_ENGAGEMENT_CLOUD_REG].[GTM].[tblFormulariosWebOpt] as b
on a.OptyId = b.OpportunityVO_Id_c

where a.CreationDate &gt;= CONVERT(varchar,DATEADD(DAY,-25,getDate()),23)
and StatusCode != 'WON'
and SalesMethod in ('GUA-Campañas Especiales','GUA-Servicios Bancarios','GUA-Tarjetas De Crédito')
and Producto_c in ('Tarjeta de Crédito','Campañas Especiales')
and (LTRIM(RTRIM(Subproducto_c)) in ('Multiproducto','Tarjeta BIO','Kit de Productos','Tarjeta Nueva Digital','Reconsideración OMA','Referido Tarjeta Nueva','Tarjeta Adicional')
    OR Subproducto_c LIKE '%Referido Tarjeta Nueva%')
and SalesChannelCd in ('WEB','SOCIAL','CHAT','BAC_BANCA_ENLINEA','BAC_BANCA_MOVIL')
and Tematica_c not in ('FORM Combo Cuenta Bancaria','FORM COMBO Extra TV','FORM CB Genérico')

) as tt
PIVOT(
	
	MIN([Valor_c])
	FOR [Nombre_c] IN ([CAMPANA],[MEDIO],[LANDING PAGE], [URL ACTUAL],[CLICK ID])

) as P</property>
