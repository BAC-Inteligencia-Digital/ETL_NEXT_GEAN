                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT  

CASE WHEN TT.StatusCode = 'CONVERTED' THEN T2.OpptyNumber ELSE TT.LeadNumber END AS ID_OPORTUNIDAD,
CASE WHEN TT.StatusCode = 'CONVERTED' THEN T2.OpptyNumber ELSE TT.LeadNumber END AS ID_OPORTUNIDAD2,
CASE WHEN TT.StatusCode = 'CONVERTED' THEN cast(T2.CreationDate as date) ELSE cast(TT.CreationDate as date) END AS FECHA_CREADO, 
CASE WHEN TT.StatusCode = 'CONVERTED' THEN cast(T2.LastUpdateDate as date) ELSE cast(TT.LastUpdateDate as date) END AS FECHA_MODIFICADO,
CASE WHEN TT.StatusCode = 'CONVERTED' THEN MONTH(T2.LastUpdateDate) ELSE MONTH(TT.LastUpdateDate) END AS MES_MODIFICACION, 
CASE WHEN TT.StatusCode = 'CONVERTED' THEN YEAR(T2.LastUpdateDate) ELSE YEAR(TT.LastUpdateDate) END AS AÑO_MODIFICACION,
'bacid' as METODO,

CASE WHEN (TT.StatusCode = 'CONVERTED' AND T2.StatusCode = 'WON') THEN 'Ganado'
     WHEN (TT.StatusCode = 'CONVERTED' AND T2.StatusCode = 'LOST') THEN 'Perdido'
	 WHEN (TT.StatusCode = 'CONVERTED' AND T2.StatusCode = 'OPEN') THEN 'En proceso'

	 WHEN TT.StatusCode = 'UNQUALIFIED' THEN 'En proceso'
	 WHEN TT.StatusCode = 'RETIRED' THEN 'Perdido' END AS ESTADO,

CASE WHEN TT.StatusCode = 'CONVERTED' THEN T2.ReasonWonLostCode
     WHEN TT.StatusCode = 'RETIRED' THEN 'Rechazado'
	 WHEN TT.StatusCode = 'UNQUALIFIED' THEN 'Prospecto' END AS 'RAZON',

CASE WHEN TT.StatusCode = 'CONVERTED' THEN T2.ReasonWonLostCode
     WHEN TT.StatusCode = 'RETIRED' THEN 'Rechazado'
	 WHEN TT.StatusCode = 'UNQUALIFIED' THEN 'Prospecto' END AS 'ETAPA_ACTUAL',

CAMPANA,
'GUA' AS PAIS,

CASE WHEN TT.ORIGEN = 'fb' THEN 'FACEBOOK FORMS'
     WHEN TT.ORIGEN = 'ig' THEN 'INSTAGRAM FORMS'
     WHEN TT.ORIGEN = 'Google' THEN 'GOOGLE FORMS' END AS CANAL,

CASE WHEN TT.MEDIO LIKE 'GT_TC%' THEN 'Tarjeta Nueva'
     WHEN TT.MEDIO LIKE '%GT_TC_BIO%' THEN 'Tarjeta BIO'
	 WHEN TT.MEDIO LIKE 'GT_CB%' THEN 'Cuenta Bancaria'
	 WHEN TT.MEDIO = 'Tarjeta de Crédito BAC' THEN 'Tarjeta Nueva'
	 WHEN TT.MEDIO = 'Solicita tu tarjeta adicional' THEN 'Tarjetas Adicionales'
	 WHEN TT.MEDIO LIKE 'GT_Adicionales%' THEN 'Tarjetas Adicionales'
	 WHEN TT.MEDIO LIKE 'GT_OMA%' THEN 'OMAS'
	 WHEN TT.MEDIO LIKE '%OMA%' THEN 'OMAS'
	 WHEN TT.MEDIO LIKE 'PF-%' THEN 'Préstamos Fácil'
	 WHEN TT.MEDIO = 'Obtén tu Efectivo Extra' THEN 'Extrafinanciamiento'
	 WHEN TT.MEDIO = 'Form Prestamo Planilla - MAYO 2023' THEN 'Préstamos Planilla'
	 WHEN TT.MEDIO LIKE '%Compass%' THEN 'Compass' 
	 ELSE TT.MEDIO END AS PRODUCTO,
	 
	 TT.CreatedBy AS USUARIO_APERTURA,
	 TT.LastUpdatedBy as USUARIO_MODIFICA,
	 
	 CASE WHEN TT.LastUpdatedBy = 'GUA_OIC_INTEG_USER' THEN 'Sí'
	      ELSE 'No' END AS 'CERRADO_POR_MOTOR_LOCAL' 

FROM (

SELECT P.LeadNumber, P.OptyNumber_c, P.LeadId, P.StatusCode, P.CanalLead_c, P.InventoryItemdescription, P.OwnerId, 
	P.CreatedBy, P.CreationDate, P.LastUpdateDate, P.PrimaryInventoryItemNumber,P.LastUpdatedBy,
	P.FuenteDinamica_c, [FECHA_TRAMITE],[CAMPANA],[MEDIO], [ORIGEN], [HORA_TRAMITE], [NOMBRE_COMPLETO], [TELEFONO_CONTACTO], [EMAIL_CONTACTO], [TELEFONO_CONTACTO_ADICIONAL]
FROM (
	SELECT a.LeadNumber, a.OptyNumber_c, a.LeadId, a.StatusCode, a.CanalLead_c, a.InventoryItemdescription, a.OwnerId, 
	a.CreatedBy,a.LastUpdatedBy, a.CreationDate, a.LastUpdateDate, a.PrimaryInventoryItemNumber,
	a.FuenteDinamica_c, b.Nombre_c, b.Valor_c
	FROM [BULK_ENGAGEMENT_CLOUD_REG].[GTM].[tblOportunidadesPotenciales] a
		left join [BULK_ENGAGEMENT_CLOUD_REG].[GTM].[tblFormulariosWebLead] b on a.LeadId = b.MklLeadVO_Id_c
	WHERE 
	a.FuenteDinamica_c='Facebook' and 
	--Ajustar el filtro de fecha según sea requerido
	a.CreationDate &gt;= '2023-11-01' 
	) as tt
PIVOT(
    MIN([Valor_c])
    FOR [Nombre_c] IN ([FECHA_TRAMITE],[CAMPANA],[MEDIO], [ORIGEN], [HORA_TRAMITE], [NOMBRE_COMPLETO], [TELEFONO_CONTACTO], [EMAIL_CONTACTO], [TELEFONO_CONTACTO_ADICIONAL])
) as P

) AS TT

LEFT JOIN [BULK_ENGAGEMENT_CLOUD_REG].[GTM].[tblOportunidades] AS T2
ON TT.LeadNumber = T2.LeadNumber_c

WHERE TT.ORIGEN in ('Google','fb','ig')
AND (TT.MEDIO LIKE 'GT_TC%'
     OR TT.MEDIO LIKE '%GT_TC_BIO%'
	 OR TT.MEDIO = 'Tarjeta de Crédito BAC'
	 OR TT.MEDIO = 'Solicita tu tarjeta adicional'
	 OR TT.MEDIO LIKE 'GT_Adicionales%'
	 OR TT.MEDIO LIKE 'GT_OMA%'
	 OR TT.MEDIO LIKE '%OMA%')</property>
