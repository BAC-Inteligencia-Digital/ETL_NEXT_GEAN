                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 

 CaseID as ID_OPT_MADRE,
 CaseID as ID_OPT_HIJA,
 'GUA' AS PAIS,
 CAST(CaseIDCreateDateTime_HourCR as date) AS FECHA_CREACION,
 CAST(CaseIDUpdateDateTime_HourCR as date) AS FECHA_MODIFICACION,
 MONTH(CaseIDUpdateDateTime_HourCR) AS MES_MODIFICACION,
 YEAR(CaseIDUpdateDateTime_HourCR) AS AÑO_MODIFICACION,
 'FORM PEGA OTC Simulado' AS TIPO_OPORTUNIDAD,
 'Web' as CANAL,

 CASE WHEN pyStatusWork in ('New','Pending','Pending-Returned','Open-Returned') THEN 'En proceso'
      ELSE 'Perdido' END AS ESTADO,

 AbandonReasonDescription AS RAZON,
 pxCreateOperator as USUARIO_APERTURA,
 pxUpdateOperator as USUARIO_MODIFICA,

 CASE WHEN pxUpdateOperator = 'SelfService.GT' THEN 'Sí' ELSE 'No' END AS CERRADO_MOTOR_LOCAL

from [BULK_CLM_REG].[pyWorkPage].[tblWorkPage]
Where Pais = 'GUA'
AND CaseID LIKE '%SX%' and ProductName = 'Credit Card'                                                                             
AND year(CaseIdCreateDateTime_HourCR) = 2022
AND pyStatusWork IN ('New','Pending','Pending-Returned','Open-Returned')</property>
