                  usesDispositions="true"
                  validateExternalMetadata="False"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT  

	CASE WHEN TT.StatusCode = 'CONVERTED' THEN T2.OpptyNumber ELSE TT.LeadNumber END AS ID_OPORTUNIDAD,
	CASE WHEN TT.StatusCode = 'CONVERTED' THEN cast(T2.CreationDate as date) ELSE cast(TT.CreationDate as date) END AS FECHA_CREADO, 
	'bacid' as METODO,
	CAMPANA AS BACID,
	CAMPANA,
	'Guatemala' AS PAIS,

	CASE WHEN  TT.ORIGEN = 'fb' THEN 'facebook'
	     WHEN  TT.ORIGEN = 'ig' THEN 'instagram'		
		 END AS FUENTE,
		 
	CASE WHEN TT.StatusCode != 'CONVERTED' then 'PROSPECTO' ELSE NULL END AS ESTADO

	FROM (

	SELECT P.LeadNumber, P.OptyNumber_c, P.LeadId, P.StatusCode, P.CanalLead_c, P.InventoryItemdescription, P.OwnerId, 
		P.CreatedBy, P.CreationDate, P.LastUpdateDate, P.PrimaryInventoryItemNumber,P.LastUpdatedBy,
		P.FuenteDinamica_c, [FECHA_TRAMITE],[CAMPANA],[MEDIO], [ORIGEN], [HORA_TRAMITE], [NOMBRE_COMPLETO], [TELEFONO_CONTACTO], [EMAIL_CONTACTO], [TELEFONO_CONTACTO_ADICIONAL]
	FROM (
		SELECT a.LeadNumber, a.OptyNumber_c, a.LeadId, a.StatusCode, a.CanalLead_c, a.InventoryItemdescription, a.OwnerId, 
		a.CreatedBy,a.LastUpdatedBy, a.CreationDate, a.LastUpdateDate, a.PrimaryInventoryItemNumber,
		a.FuenteDinamica_c, b.Nombre_c, b.Valor_c
		FROM [BULK_ENGAGEMENT_CLOUD_REG].[GTM].[tblOportunidadesPotenciales] a
			left join [BULK_ENGAGEMENT_CLOUD_REG].[GTM].[tblFormulariosWebLead] b on a.LeadId = b.MklLeadVO_Id_c
		WHERE 
		a.FuenteDinamica_c='Facebook' and 
		--Ajustar el filtro de fecha según sea requerido
		a.CreationDate &gt;= CONVERT(varchar,DATEADD(DAY,-25,getDate()),23)
		) as tt
	PIVOT(
   		MIN([Valor_c])
   		FOR [Nombre_c] IN ([FECHA_TRAMITE],[CAMPANA],[MEDIO], [ORIGEN], [HORA_TRAMITE], [NOMBRE_COMPLETO], [TELEFONO_CONTACTO], [EMAIL_CONTACTO], [TELEFONO_CONTACTO_ADICIONAL])
	) as P

	) AS TT

	LEFT JOIN [BULK_ENGAGEMENT_CLOUD_REG].[GTM].[tblOportunidades] AS T2
	ON TT.LeadNumber = T2.LeadNumber_c

	WHERE TT.ORIGEN in ('fb','ig')</property>
