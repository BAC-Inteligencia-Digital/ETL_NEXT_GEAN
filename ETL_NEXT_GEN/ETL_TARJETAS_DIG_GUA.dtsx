                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Select

OpptyNumber as ID_OPORTUNIDAD,
OpptyNumber as ID_OPORTUNIDAD_HIJA,
'GUA' AS PAIS,
CAST(CreationDate as date) as FECHA_CREACION,
CAST(LastUpdateDate as date) as FECHA_MODIFICACION,
MONTH(LastUpdateDate) AS MES_MODIFICACION,
YEAR(LastUpdateDate) AS AÑO_MODIFICACION,

CASE WHEN Subproducto_c = 'Tarjeta BIO' THEN 'Form TC BIO' ELSE Subproducto_c END AS TIPO_OPORTUNIDAD,

CASE WHEN SalesChannelCd = 'WEB' THEN 'Web'
     WHEN SalesChannelCd = 'BAC_BANCA_MOVIL' THEN 'Banca Móvil'
	 WHEN SalesChannelCd = 'BAC_BANCA_ENLINEA' THEN 'Sucursal Electronica'
	 WHEN SalesChannelCd = 'CHAT' THEN 'Chatbot'
	 WHEN (SalesChannelCd = 'SOCIAL' AND FuenteDinamica_c = 'facebook') THEN 'FACEBOOK FORMS'
	 WHEN (SalesChannelCd = 'SOCIAL' AND FuenteDinamica_c = 'google') THEN 'GOOGLE FORMS'
     END AS CANAL,

CASE WHEN StatusCode = 'LOST' THEN 'Perdido' ELSE 'En proceso' END as ESTADO,
OptyBackofficeStage_c as ETAPA_ACTUAL,
CASE WHEN SalesStage = 'Perdido' THEN 'Cerrado' ELSE 'Abierto' END AS ESTADO_ETAPA_ACTUAL,

CASE  WHEN ReasonWonLostCode = 'GOOD_LEAD' THEN 'Buena oportunidad potencial'
 WHEN ReasonWonLostCode = 'GANADA_CP' THEN 'Colocación de Producto'
 WHEN ReasonWonLostCode = 'GANADA_G' THEN 'Ganado'
 WHEN ReasonWonLostCode = 'PERDIDA_CIF' THEN 'Cliente Interesado a Futuro'
 WHEN ReasonWonLostCode = 'PERDIDA_CNC' THEN 'Cliente No Califica'
 WHEN ReasonWonLostCode = 'PERDIDA_CNF' THEN 'Cliente No Califica'
 WHEN ReasonWonLostCode = 'PERDIDA_CNDD' THEN 'Cliente No Desea más Deudas'
 WHEN ReasonWonLostCode = 'PERDIDA_CNDL' THEN 'Cliente No Desea ser Llamado'
 WHEN ReasonWonLostCode = 'PERDIDA_CNL' THEN 'Cliente No Localizado'
 WHEN ReasonWonLostCode = 'PERDIDA_CRC' THEN 'Cliente Rechaza Contrato'
 WHEN ReasonWonLostCode = 'PERDIDA_CRP' THEN 'Cliente Rechaza Propuesta'
 WHEN ReasonWonLostCode = 'PERDIDA_CRT' THEN 'Cliente Rechaza Tramite'
 WHEN ReasonWonLostCode = 'PERDIDA_CNTP' THEN 'Cliente Ya Tiene Producto'
 WHEN ReasonWonLostCode = 'PERDIDA_DBE' THEN 'De Baja-Error ó Mantenimiento'
 WHEN ReasonWonLostCode = 'PERDIDA_PC' THEN 'Políticas de Crédito'
 WHEN ReasonWonLostCode = 'PERDIDA_TI' THEN 'Rechaza por Tasa de Interés'
 WHEN ReasonWonLostCode = 'PERDIDA_SO' THEN 'Sin Ofrecimiento'
 WHEN ReasonWonLostCode = 'PERDIDA_V' THEN 'Vencido'
 ELSE ReasonWonLostCode END as RAZON,

CreatedBy AS USUARIO_APERTURA,
LastUpdatedBy AS USUARIO_MODIFICA,
CASE WHEN LastUpdatedBy IN ('GT_cargas.rest','GUA_OIC_INTEG_USER','GUA_TKN_INTEG_USER') THEN 'Sí' ELSE 'No' END AS CERRADO_POR_MOTOR_LOCAL,

CASE WHEN (Subproducto_c IN ('Tarjeta Nueva Digital','Referido Tarjeta Nueva','Kit de Productos') AND Tematica_c NOT IN ('FORM Adicionales Web Dig','FORM ADI Digital BEL','Facebook Forms Adicionales')) THEN 'Tarjeta Nueva' 
     WHEN (Subproducto_c LIKE '%Referido Tarjeta Nueva%' AND Tematica_c NOT IN ('FORM Adicionales Web Dig','FORM ADI Digital BEL','Facebook Forms Adicionales')) THEN 'Tarjeta Nueva' 
     WHEN Subproducto_c IN ('Reconsideración OMA') THEN 'OMAS'
	 WHEN Subproducto_c IN ('Tarjeta Adicional') THEN 'Tarjetas Adicionales'
	 
	 WHEN (Subproducto_c IN ('Multiproducto') AND Tematica_c in ('FORM TC Student','FORM Combo Tarjeta de Crédito','FORM Iniciativas TC','FORM TC Amex','FORM TC Amex Digital')) THEN 'Tarjeta Nueva'
	 WHEN (Subproducto_c IN ('Multiproducto') AND Tematica_c in ('FORM Combo OMAS')) THEN 'OMAS'
	 WHEN (Subproducto_c IN ('Multiproducto') AND Tematica_c in ('FORM Combo Tarjeta Adicional','FORM ADI Digital BEL')) THEN 'Tarjetas Adicionales'

	 WHEN (Subproducto_c LIKE '%Tarjeta Nueva%' AND Tematica_c in ('FORM Adicionales Web Dig','FORM ADI Digital BEL','Facebook Forms Adicionales')) THEN 'Tarjetas Adicionales'
	 WHEN (Subproducto_c = 'Kit de Productos' AND Tematica_c in ('Facebook Forms Adicionales')) THEN 'Tarjetas Adicionales'

	 WHEN Subproducto_c IN ('Multiproducto') THEN Tematica_c

	 ELSE Subproducto_c END as TIPO_PRODUCTO,

	 Tematica_c

from [BULK_ENGAGEMENT_CLOUD_REG].[GTM].[tblOportunidades] 

where CreationDate &gt;= '2023-03-01'
and StatusCode != 'WON'
and SalesMethod in ('GUA-Campañas Especiales','GUA-Servicios Bancarios','GUA-Tarjetas De Crédito')
and Producto_c in ('Tarjeta de Crédito','Campañas Especiales')
and (LTRIM(RTRIM(Subproducto_c)) in ('Multiproducto','Tarjeta BIO','Kit de Productos','Tarjeta Nueva Digital','Reconsideración OMA','Referido Tarjeta Nueva','Tarjeta Adicional')
    OR Subproducto_c LIKE '%Referido Tarjeta Nueva%')
and SalesChannelCd in ('WEB','SOCIAL','CHAT','BAC_BANCA_ENLINEA','BAC_BANCA_MOVIL')
and Tematica_c not in ('FORM Combo Cuenta Bancaria','FORM COMBO Extra TV','FORM CB Genérico','FORM Refiere Una Cuenta','FORM Cta Alcanza','Google Forms Cuenta','Facebook Forms Cuenta','FORM Tarjetas Duo producto DIG','FORM Mujeres +')</property>
