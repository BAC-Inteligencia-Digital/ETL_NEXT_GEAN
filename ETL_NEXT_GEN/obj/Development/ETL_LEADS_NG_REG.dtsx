                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT IdOportunidad as ID_OPORTUNIDAD,'CRI' as PAIS, 'Ganado' as ESTADO,

'SERVER CR' as ORIGEN,

YEAR(FechaIngreso) as ANIO_MODIFICACION, MONTH(FechaIngreso) as MES_MODIFICACION, cast(FechaCreado as date) as FECHA_CRE,
cast(FechaIngreso as date) as FECHA_MODIFICACION,

Canal as CANAL,

TipoOportunidad as TIPO_OPORTUNIDAD, 'PrÃ©stamos Personales' as TIPO_PRODUCTO

FROM
(

SELECT *
FROM 
(
/*Liga la colocaciÃ³n de pp con oportunidades en los 3 meses anteriores con la oportunidad mÃ¡s cercana a la venta eliminando duplicados*/
SELECT 
	 DimClienteID
	,TipoOportunidad
	,IdOportunidad
	,Canal
	,NumeroDeOperacion
	,DescripcionTipoOperacion
	,MontoDolarizado
	,FechaColocacion
	,FechaCreado
	,FechaIngreso
	,ROW_NUMBER() OVER(PARTITION BY IdOportunidad ORDER BY DATEDIFF(DAY,FechaCreado,FechaIngreso) ASC, EtapaDeVenta) AS Enumerador2
FROM 
(
/*Liga la colocaciÃ³n de pp con oportunidades en los 3 meses anteriores, se pueden duplicar o asignar la operaciÃ³n varias veces en un lapso de tiempo*/
SELECT 
	 l.DimClienteID
	,p.DimFechaID as FechaColocacion
	,l.TipoOportunidad
	,l.IdOportunidad
	,l.EtapaDeVenta
	,l.FechaCreado
	,convert(varchar,l.Canal) as Canal
	,p.FechaIngreso
	,p.NumeroDeOperacion
	,p.DescripcionTipoOperacion
	,p.MontoDolarizado
	,ROW_NUMBER() OVER(PARTITION BY p.DimClienteID,p.NumeroDeOperacion ORDER BY DATEDIFF(DAY,l.FechaCreado,p.FechaIngreso) ASC,l.EtapaDeVenta) AS Enumerador
FROM 
	[DWH].[dbo].[VWDim_PrestamosPersonales] p
	INNER JOIN [DWH].[dbo].[VWHec_DigitalLeads] l 
		ON l.DimClienteID = p.DimClienteID
	INNER JOIN [DWH].[dbo].[VWHec_DigitalForms] f
		ON l.IdOportunidad = f.IdOportunidad
WHERE 
	l.DimClienteID &lt;&gt; -1 AND
	p.DimFechaID &gt;= 20220101 AND
	l.DimDigitalCatalogoProductoID = 8 AND
	p.NumeroDeOperacion &lt;&gt; 0 AND
	(DATEDIFF(DAY,FechaCreado,FechaIngreso) &gt;= 0 AND FechaIngreso &lt;= DATEADD(MM,3,FechaCreado))
) a
WHERE 
	Enumerador = 1 
	) b
	WHERE 
		Enumerador2 = 1 and 
		FechaColocacion &gt;= 20230101

) as tt</property>
