                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    OPT.ROW_ID AS ID_OPT,
    OPT.PAIS as PAIS,
	CAST(OPT.CREATED  AS date) as FECHA_CREADO,
	CAST(OPT.LAST_UPD AS DATE) as FECHA_MODIFICACION,
	MONTH(OPT.LAST_UPD) as MES_MODIFICACION, 
	YEAR(OPT.LAST_UPD) as AÃO_MODIFICACION,
	OPT.STATUS_CD as ESTADO_OPORTUNIDAD,
	ACT.TODO_CD AS ETAPA_ANTERIOR,
	T8.NAME AS ETAPA_ACTUAL,
	
	CASE WHEN T8.STAGE_STATUS_CD != 'Abierto' THEN 'Cerrado' ELSE T8.STAGE_STATUS_CD END AS ESTADO_ETAPA_ACTUAL, 
	
	OPT.CHANNEL_TYPE_CD as CANAL,
    
	CASE WHEN (OPT.REASON_WON_LOST_CD IS NULL AND OPT.STATUS_CD = 'Perdido') THEN T8.NAME
	     ELSE OPT.REASON_WON_LOST_CD END AS RAZÃN,
	
	OPT.OPTY_CD AS TIPO_OPORTUNIDAD,
	U.LOGIN AS USUARIO_APERTURA,
	M.LOGIN AS USUARIO_MODIFICA,
	'SIEBEL' AS ORIGEN
	 
FROM DWSiebel.CRI.S_OPTY OPT
	inner JOIN DWSiebel.CRI.S_SALES_METHOD MV on OPT.SALES_METHOD_ID = MV.ROW_ID
	inner JOIN DWSiebel.CRI.S_USER U ON OPT.CREATED_BY = U.PAR_ROW_ID
	inner JOIN DWSiebel.CRI.S_USER M ON OPT.LAST_UPD_BY = M.PAR_ROW_ID
	LEFT OUTER JOIN DWSiebel.CRI.S_STG T8 ON OPT.CURR_STG_ID = T8.ROW_ID

	LEFT JOIN (
		select * from (
		select LAST_UPD,ROW_NUMBER() OVER(PARTITION BY OPTY_ID ORDER BY LAST_UPD DESC) AS Fila,OPTY_ID,TODO_CD
		from DWSiebel.[CRI].[S_EVT_ACT]
		WHERE OPTY_ID IS NOT NULL
		  AND TODO_CD IS NOT NULL
		  AND YEAR(CREATED) &gt;= 2022 
		) as x where x.Fila=2

	) AS ACT ON OPT.ROW_ID = ACT.OPTY_ID 

WHERE (YEAR(OPT.CREATED) &gt;= 2023 OR YEAR(OPT.LAST_UPD) &gt;= 2023)
	AND OPT.CHANNEL_TYPE_CD IN ('Web','Sucursal Electronica','iBAC','iBAC Movil','Sucursal Movil','Banca Movil')
	AND U.LOGIN = 'SADMIN'
	AND OPT.OPTY_CD IN ('FORM PRESTAMOS PERSON EN LINEA',
'Form Prestamos Whatsapp',
'FORM DECA PREST. PERSONAL SEC')</property>
