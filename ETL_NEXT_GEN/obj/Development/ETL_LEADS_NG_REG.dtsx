                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  IdOportunidad as ID_OPORTUNIDAD,'CRI' as PAIS, FechaInicio as FECHA_CREACION, 
        FechaApertura AS FECHA_MODIFICACION, MONTH(FechaApertura) as MES_MODIFICACION, YEAR(FechaApertura) as ANIO_MODIFICACION,
		'Ganado' as ESTADO, Canal AS CANAL, TipoOportunidad AS TIPO_OPORTUNIDAD, 'SERVER CR' as ORIGEN,
		'PrÃ©stamos Hipotecarios' as TIPO_PRODUCTO

		FROM (
SELECT

        cast(convert(varchar, Fechainicio, 120) as date) as Fechainicio

       ,DimClienteID

       ,DimOperacionCreditoID

       ,TipoOportunidad

       ,Canal

       ,IdOportunidad

       ,AgrupacionCartera

       ,NumeroOperacion

       ,FechaApertura

       ,MontoOriginalDolarizado

FROM

(

SELECT

        l.DimFechaID as Fechainicio

       ,l.DimClienteID

       ,p.DimOperacionCreditoID

       ,l.TipoOportunidad

       ,l.IdOportunidad

       ,l.FechaCreado

       ,l.Canal

       ,p.AgrupacionCartera

       ,p.NumeroOperacion

       ,p.FechaApertura

       ,p.MontoOriginalDolarizado

       ,ROW_NUMBER() OVER(PARTITIon BY p.NumeroOperacion,p.DimOperacionCreditoID ORDER BY DATEDIFF(DAY,l.FechaCreado,p.FechaApertura)) AS Enumerador

FROM

       [DWH].[dbo].[VWDim_OperacionCredito] p

       INNER JOIN[DWH].[dbo].[VWHec_DigitalLeads] l

             ON l.DimClienteID = p.DimClienteID

WHERE

       l.DimClienteID &lt;&gt; -1 AND

       l.DimDigitalCatalogoProductoID = 6 AND

       (AgrupacionCartera = 'HIPOTECARIO' OR AgrupacionCartera = 'CONSUMO-HIPOTECARIO') AND 

       FechaApertura &gt;= '2023-01-01' AND

       (DATEDIFF(DAY,FechaCreado,FechaApertura) &gt;= 0 AND FechaApertura &lt;= DATEADD(MM,6,FechaCreado))

) a

WHERE

       Enumerador = 1

) as TT</property>
