                <component
                  refId="Package\LEADS_GUATEMALA\OBTENER LEADS\LEADS CHATBOT"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="LEADS CHATBOT"
                  usesDispositions="true"
                  validateExternalMetadata="False"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">---- LEADS PERDIDOS/EN PROCESO ----

SELECT 
    OPT.ROW_ID AS ID_OPT_MADRE,
	OPT.ROW_ID AS ID_OPT_HIJA,
    OPT.PAIS as PAIS,
	CAST(OPT.CREATED  AS date) as FECHA_CREADO,
	CAST(OPT.LAST_UPD AS DATE) as FECHA_MODIFICACION,
	MONTH(OPT.LAST_UPD) as MES_MODIFICACION, 
	YEAR(OPT.LAST_UPD) as AÃO_MODIFICACION,
	
	CASE WHEN OPT.STATUS_CD = 'Generado' THEN 'En proceso' ELSE OPT.STATUS_CD END as ESTADO_OPORTUNIDAD,
	
	ACT.TODO_CD AS ETAPA_ANTERIOR,
	T8.NAME AS ETAPA_ACTUAL,
	
	CASE WHEN T8.STAGE_STATUS_CD != 'Abierto' THEN 'Cerrado' ELSE T8.STAGE_STATUS_CD END AS ESTADO_ETAPA_ACTUAL, 
	
	CASE WHEN SRC_TYPE_CD = 'SMS' THEN CONCAT('Chatbot',' ','SMS')
	     WHEN SRC_TYPE_CD = 'EMAIL' THEN CONCAT('Chatbot',' ','MAIL')
		 WHEN SRC_TYPE_CD = 'Televentas OriginaciÃ³n' THEN CONCAT('Chatbot',' ','SMS') 
		 ELSE CONCAT('Chatbot',' ','SMS')  END AS CANAL,
    
	CASE WHEN (OPT.REASON_WON_LOST_CD IS NULL AND OPT.STATUS_CD = 'Perdido') THEN T8.NAME
	     ELSE OPT.REASON_WON_LOST_CD END AS RAZÃN,
	
	OPT.OPTY_CD AS TIPO_OPORTUNIDAD,

	CASE WHEN OPT.OPTY_CD LIKE '%Compass%' then 'Compass'
	WHEN OPT.OPTY_CD LIKE '%Ptmo FÃ¡cil CHAT%' then 'Extrafinanciamiento'
	WHEN OPT.OPTY_CD LIKE '%Extrafinanciamiento%' then 'Extrafinanciamiento'
	WHEN OPT.OPTY_CD LIKE '%Prestamo Planilla%' then 'PrÃ©stamo Planilla'
	WHEN OPT.OPTY_CD LIKE '%OMA Pref%' then 'OMAS' end AS PRODUCTO,

	U.LOGIN AS USUARIO_APERTURA,
	M.LOGIN AS USUARIO_MODIFICA,
	'SIEBEL' AS ORIGEN
	 
FROM DWSiebel.GUA.S_OPTY OPT
	inner JOIN DWSiebel.GUA.S_SALES_METHOD MV on OPT.SALES_METHOD_ID = MV.ROW_ID
	inner JOIN DWSiebel.GUA.S_USER U ON OPT.CREATED_BY = U.PAR_ROW_ID
	inner JOIN DWSiebel.GUA.S_USER M ON OPT.LAST_UPD_BY = M.PAR_ROW_ID
	LEFT OUTER JOIN DWSiebel.GUA.S_STG T8 ON OPT.CURR_STG_ID = T8.ROW_ID

	LEFT JOIN (
		select * from (
		select LAST_UPD,ROW_NUMBER() OVER(PARTITION BY OPTY_ID ORDER BY LAST_UPD DESC) AS Fila,OPTY_ID,TODO_CD
		from DWSiebel.[GUA].[S_EVT_ACT]
		WHERE OPTY_ID IS NOT NULL
		  AND TODO_CD IS NOT NULL
		  AND YEAR(CREATED) &gt;= 2022 
		) as x where x.Fila=2

	) AS ACT ON OPT.ROW_ID = ACT.OPTY_ID 

WHERE YEAR(OPT.CREATED) &gt;= 2023
	--AND U.LOGIN = 'SADMIN'
	AND OPT.STATUS_CD NOT IN ('Ganado')
	AND OPT.OPTY_CD IN ('Chatbot Compass','Chatbot Extrafinanciamiento','Chatbot Prestamo Planilla','FORM Referido Ptmo FÃ¡cil CHAT') 

UNION ALL 

---- LEADS GANADOS ----

SELECT 
    OPT.ROW_ID AS ID_OPT_MADRE,
	OPT.ROW_ID AS ID_OPT_HIJA,
    OPT.PAIS as PAIS,
	CAST(OPT.CREATED  AS date) as FECHA_CREADO,
	CAST(OPT.LAST_UPD AS DATE) as FECHA_MODIFICACION,
	MONTH(OPT.LAST_UPD) as MES_MODIFICACION, 
	YEAR(OPT.LAST_UPD) as AÃO_MODIFICACION,
	CASE WHEN OPT.STATUS_CD = 'Generado' THEN 'En proceso' ELSE OPT.STATUS_CD END as ESTADO_OPORTUNIDAD,
	ACT.TODO_CD AS ETAPA_ANTERIOR,
	T8.NAME AS ETAPA_ACTUAL,
	
	CASE WHEN T8.STAGE_STATUS_CD != 'Abierto' THEN 'Cerrado' ELSE T8.STAGE_STATUS_CD END AS ESTADO_ETAPA_ACTUAL, 
	
	CASE WHEN SRC_TYPE_CD = 'SMS' THEN CONCAT('Chatbot',' ','SMS')
	     WHEN SRC_TYPE_CD = 'EMAIL' THEN CONCAT('Chatbot',' ','MAIL')
		 WHEN SRC_TYPE_CD = 'Televentas OriginaciÃ³n' THEN CONCAT('Chatbot',' ','SMS') 
		 ELSE CONCAT('Chatbot',' ','SMS')  END AS CANAL,
    
	CASE WHEN (OPT.REASON_WON_LOST_CD IS NULL AND OPT.STATUS_CD = 'Perdido') THEN T8.NAME
	     ELSE OPT.REASON_WON_LOST_CD END AS RAZÃN,
	
	OPT.OPTY_CD AS TIPO_OPORTUNIDAD,

	CASE WHEN OPT.OPTY_CD LIKE '%Compass%' then 'Compass'
	WHEN OPT.OPTY_CD LIKE '%Ptmo FÃ¡cil CHAT%' then 'Extrafinanciamiento'
	WHEN OPT.OPTY_CD LIKE '%Extrafinanciamiento%' then 'Extrafinanciamiento'
	WHEN OPT.OPTY_CD LIKE '%Prestamo Planilla%' then 'PrÃ©stamo Planilla'
	WHEN OPT.OPTY_CD LIKE '%OMA Pref%' then 'OMAS' end AS PRODUCTO,

	U.LOGIN AS USUARIO_APERTURA,
	M.LOGIN AS USUARIO_MODIFICA,
	'SIEBEL' AS ORIGEN
	 
FROM DWSiebel.GUA.S_OPTY OPT
	inner JOIN DWSiebel.GUA.S_SALES_METHOD MV on OPT.SALES_METHOD_ID = MV.ROW_ID
	inner JOIN DWSiebel.GUA.S_USER U ON OPT.CREATED_BY = U.PAR_ROW_ID
	inner JOIN DWSiebel.GUA.S_USER M ON OPT.LAST_UPD_BY = M.PAR_ROW_ID
	LEFT OUTER JOIN DWSiebel.GUA.S_STG T8 ON OPT.CURR_STG_ID = T8.ROW_ID

	LEFT JOIN (
		select * from (
		select LAST_UPD,ROW_NUMBER() OVER(PARTITION BY OPTY_ID ORDER BY LAST_UPD DESC) AS Fila,OPTY_ID,TODO_CD
		from DWSiebel.[GUA].[S_EVT_ACT]
		WHERE OPTY_ID IS NOT NULL
		  AND TODO_CD IS NOT NULL
		  AND YEAR(CREATED) &gt;= 2022 
		) as x where x.Fila=2

	) AS ACT ON OPT.ROW_ID = ACT.OPTY_ID 

WHERE OPT.LAST_UPD &gt;= '2023-04-01'
	--AND U.LOGIN = 'SADMIN'
	AND OPT.STATUS_CD IN ('Ganado')
	AND OPT.OPTY_CD IN ('Chatbot Compass','Chatbot Extrafinanciamiento','Chatbot Prestamo Planilla','FORM Referido Ptmo FÃ¡cil CHAT')</property>
