---- LEADS GANADOS (OTROS CUENTA BANCARIA) ----

SELECT

    MADRE.ROW_ID AS ID_OPT_MADRE,
	HIJA.ROW_ID AS ID_OPT_HIJA,
    MADRE.PAIS as PAIS,
	CAST(MADRE.CREATED  AS date) as FECHA_CREADO,
	CAST(HIJA.LAST_UPD AS DATE) as FECHA_MODIFICACION,
	MONTH(HIJA.LAST_UPD) as MES_MODIFICACION, 
	YEAR(HIJA.LAST_UPD) as AÃO_MODIFICACION,
	HIJA.STATUS_CD as ESTADO_OPORTUNIDAD,
	ACT.TODO_CD AS ETAPA_ANTERIOR,
	T8.NAME AS ETAPA_ACTUAL,
	
	CASE WHEN T8.STAGE_STATUS_CD != 'Abierto' THEN 'Cerrado' ELSE T8.STAGE_STATUS_CD END AS ESTADO_ETAPA_ACTUAL, 
	
	MADRE.CHANNEL_TYPE_CD as CANAL,
    
	CASE WHEN (HIJA.REASON_WON_LOST_CD IS NULL AND HIJA.STATUS_CD = 'Perdido') THEN T8.NAME
	     ELSE HIJA.REASON_WON_LOST_CD END AS RAZÃN,
	
	HIJA.OPTY_CD AS TIPO_OPORTUNIDAD,
	U.LOGIN AS USUARIO_APERTURA,
	M.LOGIN AS USUARIO_MODIFICA,
	'SIEBEL' AS ORIGEN

FROM DWSiebel.GUA.S_OPTY MADRE
LEFT JOIN DWSiebel.GUA.S_OPTY HIJA on MADRE.ROW_ID=HIJA.PAR_OPTY_ID
LEFT OUTER JOIN DWSiebel.GUA.S_SALES_METHOD MV on HIJA.SALES_METHOD_ID = MV.ROW_ID
LEFT OUTER JOIN DWSiebel.GUA.S_USER U ON MADRE.CREATED_BY = U.PAR_ROW_ID
LEFT OUTER JOIN DWSiebel.GUA.S_USER M ON HIJA.LAST_UPD_BY = M.PAR_ROW_ID
LEFT OUTER JOIN DWSiebel.GUA.S_STG T8 ON HIJA.CURR_STG_ID = T8.ROW_ID

LEFT JOIN (
		select * from (
		select LAST_UPD,ROW_NUMBER() OVER(PARTITION BY OPTY_ID ORDER BY LAST_UPD DESC) AS Fila,OPTY_ID,TODO_CD
		from DWSiebel.[GUA].[S_EVT_ACT]
		WHERE OPTY_ID IS NOT NULL
		  AND TODO_CD IS NOT NULL
		  AND YEAR(CREATED) &gt;= 2022 
		) as x where x.Fila=2

	) AS ACT ON HIJA.ROW_ID = ACT.OPTY_ID


