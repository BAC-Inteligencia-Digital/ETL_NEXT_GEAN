                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT ID_OPORTUNIDAD, FECHA_CREACION, PAIS, [URL ACTUAL], 

CASE WHEN [CAMPANA] IS NULL THEN ''
     WHEN [CAMPANA] = '-- NO DATA --' THEN ''
	 WHEN [CAMPANA] LIKE 'HON%' THEN REPLACE([CAMPANA], ',', '/')
	 ELSE [CAMPANA] END AS BACID,
	 
	 CASE WHEN [MEDIO] IS NULL THEN 'orgÃ¡nicas'
	      WHEN [CAMPANA] IS NULL THEN 'orgÃ¡nicas'
		  WHEN [CAMPANA] = '-- NO DATA --' THEN 'orgÃ¡nicas'
		  ELSE [MEDIO] END AS MEDIO,
	 
CASE WHEN [CAMPANA] IS NULL THEN ''
     WHEN [CAMPANA] = '-- NO DATA --' THEN ''
	 WHEN [CAMPANA] LIKE 'HON%' THEN REPLACE([Campana], ',', '/')
	 ELSE [CAMPANA] END AS CAMPANA, 
	 
CASE WHEN [LANDING PAGE] IS NULL THEN '' 
     WHEN [LANDING PAGE] = '-- NO DATA --' THEN '' 
	 ELSE [LANDING PAGE] END AS LANDINGPAGE

FROM (

Select

a.OpptyNumber as ID_OPORTUNIDAD,
CAST(a.CreationDate as date) as FECHA_CREACION,
'Honduras' as PAIS,
b.Nombre_c,
b.Valor_c

from [BULK_ENGAGEMENT_CLOUD_REG].[HND].[tblOportunidades] as a
left join [BULK_ENGAGEMENT_CLOUD_REG].[HND].[tblFormulariosWebOpt] as b
on a.OptyId = b.OpportunityVO_Id_c

where
A.CreatedBy in ('HON_OIC_INTEG_USER','HON_TKN_INTEG_USER') and
A.Subproducto_c in ('AfiliaciÃ³n Nueva','Compra Click','MiPOS','Plan de PensiÃ³n Centro','Plan de PensiÃ³n Norte',
                  'Cargos o Pagos AutomÃ¡ticos HON', 'Mastercard ClÃ¡sica Local','Compass','Certificado de DepÃ³sito a Plazo') and
(A.CreationDate &gt;= CONVERT(varchar,DATEADD(DAY,-25,getDate()),23) OR A.LastUpdateDate &gt;= CONVERT(varchar,DATEADD(DAY,-25,getDate()),23))

) as tt
PIVOT(
	
	MIN([Valor_c])
	FOR [Nombre_c] IN ([CAMPANA],[MEDIO],[LANDING PAGE], [URL ACTUAL])

) as P</property>
