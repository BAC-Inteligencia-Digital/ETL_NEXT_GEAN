                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT A.CaseID AS ID_OPORTUNIDAD,
       'ESA' AS PAIS,
	   CAST(A.CaseIDCreateDateTime_HourCR AS DATE) AS FECHA_CREACION,
	   CAST(A.CaseIDUpdateDateTime_HourCR AS DATE) AS FECHA_MODIFICACION,
	   YEAR(A.CaseIDUpdateDateTime_HourCR) AS A√ëO_MODIFICACION,
	   MONTH(A.CaseIDUpdateDateTime_HourCR) AS MES_MODIFICACION,
	   'FORM PEGA OTC Simulado' AS TIPO_OPORTUNIDAD,

	   CASE WHEN A.CaseID LIKE 'SX%' THEN 'Web'
	        WHEN A.CaseID LIKE 'BE%' THEN 'Sucursal Electr√≥nica' END AS CANAL,

	   CASE WHEN A.pyStatusWork = 'Resolved-Approved' THEN 'Ganado'
            WHEN A.pyStatusWork IN ('Resolved-Abandoned','Resolved-Rejected','Resolved-NotApplies','Resolved-Bridger','Resolved-Error') THEN 'Perdido'
			WHEN A.pyStatusWork IN ('New','Pending','Retry-In-Process','Open-Returned','Administrator-Review','Pending-RequestInitiation',
			     'Verification-InProgress') THEN 'En proceso'
				 END AS ESTADO,

	   CASE WHEN A.pyStatusWork = 'Resolved-Approved' THEN 'Ganado'
	        WHEN (A.pyStatusWork = 'Resolved-Rejected' AND A.pxUpdateOperator = 'SelfService.SV') THEN 'Rechazado por OMDM'
			WHEN (A.pyStatusWork = 'Resolved-Rejected' AND A.AbandonReasonCode is null) THEN 'Rechazado'
			WHEN (A.pyStatusWork = 'Resolved-NotApplies' AND A.AbandonReasonCode is null) THEN 'No aplica'
			WHEN (A.pyStatusWork = 'Resolved-Abandoned' AND A.AbandonReasonCode is null) THEN 'Abandono por el cliente'
			ELSE A.AbandonReasonCode END AS RAZON,

       A.pxCreateOperator AS USUARIO_APERTURA,
	   A.pxUpdateOperator AS USUARIO_MODIFICA,

	   'Tarjeta Nueva' AS TIPO_PRODUCTO,

	   A.COMCif AS CIF_COM,
	   A.AccountNumber AS NUMERO_TARJETA,

	   CASE WHEN B.NewBrandCode = 'VSA' THEN 'VISA'
	        WHEN B.NewBrandCode = 'MCD' THEN 'MASTERCARD'
			WHEN B.NewBrandCode = 'AMX' THEN 'AMEX' END AS MARCA,

	   CASE WHEN B.NewClassificationCode = 'CLS' THEN 'CLASICA'
	        WHEN B.NewClassificationCode = 'PLT' THEN 'PLATINO'
			WHEN B.NewClassificationCode = 'GLD' THEN 'DORADA'
			WHEN B.NewClassificationCode = 'BCK' THEN 'BLACK' END AS COLOR,

	    B.LoyaltyPlanName AS PROGRAMA_LEALTAD, B.ClientLimit AS MONTO_LIMITE, 1 AS FLAG_CLEAN

FROM [BULK_CLM_REG].[pyWorkPage].[tblWorkPage] AS A
LEFT JOIN [BULK_CLM_REG].[OTC].[tblCardsSelected] AS B
ON A.CaseID = B.CaseID
WHERE A.Pais = 'SAL'
AND (A.CaseID LIKE 'SX%' OR A.CaseID LIKE 'BE%')
AND (YEAR(A.CaseIDCreateDateTime_HourCR) &gt;= 2024 OR  YEAR(A.CaseIDUpdateDateTime_HourCR) &gt;= 2024)</property>
