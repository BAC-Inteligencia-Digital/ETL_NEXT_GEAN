                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT A.ID_OPORTUNIDAD FROM [TRANSFORMACION].[dbo].[MCKINSEY_LEADS_TARJETAS_ESA] AS A
LEFT JOIN [CANALES].[dbo].[BV_Webforms_Historico_BACID_MySQL] AS B
ON A.ID_OPORTUNIDAD = B.id_oportunidad COLLATE SQL_Latin1_General_CP1_CI_AS
AND B.fecha_insercion &gt;= CONVERT(varchar,DATEADD(DAY,-15,getDate()),23)
WHERE FECHA_CREACION &gt;= CONVERT(varchar,DATEADD(DAY,-15,getDate()),23)
AND (A.ID_OPORTUNIDAD LIKE 'SX%' or A.ID_OPORTUNIDAD LIKE 'BE%') 
AND B.id_oportunidad IS NULL
GROUP BY A.ID_OPORTUNIDAD</property>
